SCREEN s_steuern_selbsttest()
{
  ftextout("Status Selbsttest lesen",1,0,1,0);
  ftextout("",3,0,0,0);
  INPAapiJob(sgbd,"STEUERN_ROUTINE","ARG;SELBSTTEST;STR;0x04","");		//Selbsttest aller Routinen 0x04
  INPAapiCheckJobStatus("OKAY");
  delay(1000);																						//Wartezeit nach STEUERN erforderlich???? 
  INPAapiJob(sgbd,"STEUERN_ROUTINE","ARG;SELBSTTEST;RRR","");
  INPAapiCheckJobStatus("OKAY");

  /*LINE("Status Selbsttest","")
  {*/
	 ftextout("Selbsttest Status: ",5,5,0,1);
	 
	 ganz_zahl = 0;
	                                                            		//Status - Zähler auf Null setzen
	 INPAapiResultInt(ganz_zahl,"STAT_SELBSTTEST",1);                       //Ergebnisstatus abfragen solange Test läuft
	 
	 logik_var = FALSE;																				//Status - Zähler auf 1 (Test läuft) setzen                                                         
	 settimer(1,5000);
	 
	 while (ganz_zahl == 1 && (logik_var == FALSE))                         //Schleife durchlaufen solange Test nicht beendet ist und Teststatus ausgeben       
	 {																									// (1 = Test läuft)
		 INPAapiResultText(text_var,"STAT_SELBSTTEST_TEXT",1,"");       //Ergebnistext ausgeben solange Test läuft
		 ftextout(text_var          , 5, 50,0,1);  
		
		 INPAapiJob(sgbd,"STEUERN_ROUTINE","ARG;SELBSTTEST;RRR","");	//STAT_SELBSTTEST  = 0
  		 INPAapiCheckJobStatus("OKAY");
		 INPAapiResultInt(ganz_zahl,"STAT_SELBSTTEST",1);               //Ergebnisstatus abfragen solange Test läuft
		 delay(200);
		 testtimer(1,logik_var);
	 }
	 
	 INPAapiResultText(text_var,"STAT_SELBSTTEST_TEXT",1,"");               //Status ausgeben wenn Test beendet
	 ftextout(text_var          , 5, 50,0,1);
	 
	 if (ganz_zahl == 3)																			//Test beendet mit Fehlern
	 {																									//FS_LESEN
	 	delay(2000);
	 	userboxclose(0);
      		userboxopen(0,8,35,5,50,"Fehlerspeicher lesen","");							
      		userboxftextout(0,"Der Fehlerspeicher wird gelesen! ... bitte warten",1,3,0,0);
		INPAapiFsMode(0xFF,"w","","",""); 													//alles aus dem FS Anzeigen und Fehlerprotokoll neu anlegen
    		INPAapiFsLesen(sgbd,"na_fs.tmp");													//FS Datei ausgeben, Dateiname: na_fs.tmp
    		userboxclose(0);
    		viewopen("na_fs.tmp","Fehlerspeicher lesen");
      		// sicherheitshalber neu starten ???
    		// start();
    	}
//  }
} 
