// ****************************
// *** Menü für LL-Erhöhung ***
// ****************************
MENU m_system_llerh()
{
  int ll_help;
  string s1;

  INIT
  {
    setmenutitle("EOL - LeerLauf");
    taste = 0;
  }
  ITEM(1,"+10")
  {
    llerh(10);
    setscreen(s_system_llerh,TRUE);
    taste = 1;
  }
  ITEM(2,"-10")
  {
    llerh(-10);
    setscreen(s_system_llerh,TRUE);
    taste = 1;
  }
  ITEM(3,"+100")
  {
    llerh(100);
    setscreen(s_system_llerh,TRUE);
    taste = 1;
  }
  ITEM(4,"-100")
  {
    llerh(-100);
    setscreen(s_system_llerh,TRUE);
    taste = 1;
  }
  ITEM(6,"Auswahl")
  {
    input_state = 2;
    while (input_state != 0)
    {
      ll_help = ll_wert;
      inputint(ll_help,"Vorgabe LL-Sollwert","Drehzahl in den Grenzen von 0 < N < 4000",0,4000);
      getinputstate(input_state);
    }
    if (input_state == 0)
    {
      ll_help = ll_help - ll_wert;

      llerh(ll_help);
      blankscreen();
      setscreen(s_system_llerh,TRUE);
      taste = 1;
    }
  }
  ITEM(9,"STOP")
  {
    INPAapiJob(sgbd,"STOP_SYSTEMCHECK_LLERH","","");
    INPAapiResultText(s1,"JOB_STATUS",1,"");
    if (s1 != "OKAY")     { messagebox("STOP_SYSTEMCHECK_LLERH",s1); stop(); }
    ll_soll=0;
  }
  ITEM(10,"Zurück") {blankscreen(); setscreen(s_system,TRUE); setmenu(m_system);}
  ITEM(20,"ENDE")   {userboxclose(0); viewclose(); exit();}
}


